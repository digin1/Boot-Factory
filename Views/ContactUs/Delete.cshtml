@model Boot_Factory.Models.ContactUs

@{
    ViewData["Title"] = "Delete Query";
}

<div class="container p-4">

    <h4>Are you sure you want to delete this query from <b>@Html.DisplayFor(model => model.Email)?</b></h4>
    <div>


        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Yes" class="btn btn-danger" /> <a asp-action="Index" class="btn btn-success">No</a>
        </form>
    </div>
</div>
